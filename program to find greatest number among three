
#include <iostream>
using namespace std;
int main() {
    int num1,num2,num3;
    cout << "Enter the first number :"<<endl;
    cin >> num1;
    cout << "Enter the second number :"<<endl;
    cin >> num2;
    cout << "Enter the third number :"<<endl;
    cin >> num3;
    if(num1>=num2 && num1>=num3){
        cout<<"this number is greater"<< num1;
    }
    if(num2>=num1 && num2>=num3){
        cout<<"this number is greater"<< num2;
    }
    if(num3>=num1 && num3>=num2){
        cout<<"this number is greater"<< num3;
    }
    return 0;
}
